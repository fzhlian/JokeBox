{
  "name": "@ohos/hvigor-arkts-base",
  "version": "6.22.3",
  "description": "hvigor-arkts-base",
  "main": "./dist/index.js",
  "scripts": {
    "patch-publish": "npm --no-git-tag-version version prepatch&&npm publish",
    "prepack": "npm run ugly-build",
    "build": "tsc -p .",
    "watch": "tsc --watch",
    "clean": "tsc --build --clean",
    "rebuild": "npm run clean&&npm run build",
    "uglify": "node uglify.mjs",
    "ugly-build": "npm run clean && npm run build && npm run uglify",
    "test": "jest",
    "coverage": "nyc --clean jest --coverage",
    "fix": "eslint ./**/*.ts --fix"
  },
  "dependencies": {
    "@ohos/hvigor-common": "6.22.3",
    "@ohos/hvigor-logger": "6.22.3",
    "fs-extra": "11.3.0",
    "log4js": "6.9.1",
    "rollup": "3.19.1-htrunk1.AllScenario.devecostudio.r8"
  },
  "bundledDependencies": [
    "rollup"
  ],
  "overrides": {
    "flatted": "3.2.7",
    "rfdc": "1.3.0"
  },
  "files": [
    "dist/cli/**/*.js",
    "dist/cli/**/*.d.ts",
    "dist/src/**/*.js",
    "dist/src/**/*.d.ts",
    "dist/index.js",
    "dist/index.d.ts",
    "lib/*"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "author": "huawei",
  "ohos": {
    "org": "huawei"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "keywords": [
    "hvigor",
    "OpenHarmony"
  ]
}
