{
  "name": "@ohos/hvigor-logger",
  "version": "6.22.3",
  "description": "hvigor logger package.",
  "main": "index.js",
  "scripts": {
    "patch-publish": "npm --no-git-tag-version version prepatch&&npm publish",
    "prepack": "npm run ugly-build",
    "build": "tsc -p .",
    "watch": "tsc --watch",
    "clean": "tsc --build --clean",
    "rebuild": "npm run clean && npm run build",
    "uglify": "node ../../scripts/uglify.js",
    "ugly-build": "npm run clean && npm run build && npm run uglify && npm run uglifypack",
    "uglifypack": "node ../../scripts/uglifypack.js",
    "test": "jest",
    "coverage": "nyc --clean jest --coverage",
    "fix": "eslint ./**/*.ts --fix"
  },
  "author": "huawei",
  "ohos": {
    "org": "huawei"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "@ohos/hvigor-common": "6.22.3",
    "fs-extra": "11.3.0",
    "typescript": "4.9.5",
    "log4js": "6.9.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.11"
  },
  "overrides": {
    "flatted": "3.2.7",
    "rfdc": "1.3.0",
    "jsonfile": "6.1.0"
  },
  "bundledDependencies": false,
  "keywords": [
    "hvigor"
  ]
}
