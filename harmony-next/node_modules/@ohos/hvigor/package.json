{
  "name": "@ohos/hvigor",
  "version": "6.22.3",
  "description": "The ohos build system cli tools.",
  "main": "./index.js",
  "scripts": {
    "patch-publish": "npm --no-git-tag-version version prepatch&&npm publish",
    "prepack": "npm run ugly-build",
    "build": "tsc -p .",
    "watch": "tsc --watch",
    "clean": "tsc --build --clean",
    "rebuild": "npm run clean && npm run build",
    "uglify": "node ../../scripts/uglify.js",
    "ugly-build": "npm run clean && npm run build && npm run uglify && npm run uglifypack",
    "uglifypack": "node ../../scripts/uglifypack.js",
    "test": "jest",
    "rollup:build": "tsc -p . && rollup -c",
    "coverage": "nyc --clean jest --coverage",
    "fix": "eslint src/**/*.ts --fix"
  },
  "author": "huawei",
  "ohos": {
    "org": "huawei"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "@ohos/hvigor-common": "6.22.3",
    "@ohos/hvigor-logger": "6.22.3",
    "typescript": "4.9.5",
    "log4js": "6.9.1",
    "fs-extra": "11.3.0",
    "commander": "13.1.0",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "semver": "7.5.4",
    "@baize/wdk": "0.7.2",
    "chokidar": "4.0.3",
    "ws": "8.14.1"
  },
  "overrides": {
    "electron-to-chromium": "1.5.13",
    "terser": "5.17.1",
    "get-intrinsic": "1.2.7",
    "flatted": "3.2.7",
    "rfdc": "1.3.0",
    "engine.io-client": "6.6.3",
    "engine.io-parser": "5.2.3",
    "glob-parent": "6.0.2",
    "node-XMLHttpRequest": "2.1.2",
    "@floating-ui/dom": "1.7.0",
    "@floating-ui/core": "1.7.0",
    "socket.io": {
      "@types/cors": "2.8.17",
      "ws": "8.17.1"
    }
  },
  "devDependencies": {
    "@types/node": "^16.11.11",
    "@types/ws": "8.2.3"
  },
  "peerDependencies": {
    "@ohos/hvigor-common": "6.22.3",
    "@ohos/hvigor-logger": "6.22.3"
  },
  "keywords": [
    "hvigor"
  ],
  "bundleDependencies": [
    "@ohos/hvigor-common",
    "@ohos/hvigor-logger",
    "typescript",
    "log4js",
    "fs-extra",
    "commander",
    "socket.io",
    "socket.io-client",
    "semver",
    "@baize/wdk",
    "chokidar",
    "ws"
  ]
}
