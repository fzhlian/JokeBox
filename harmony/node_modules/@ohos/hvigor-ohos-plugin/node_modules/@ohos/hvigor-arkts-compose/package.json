{
  "name": "@ohos/hvigor-arkts-compose",
  "version": "6.22.3",
  "description": "hvigor-arkts-compose",
  "main": "./dist/index.js",
  "scripts": {
    "patch-publish": "npm --no-git-tag-version version prepatch&&npm publish",
    "prepack": "npm run ugly-build",
    "build": "tsc -p .",
    "watch": "tsc --watch",
    "clean": "tsc --build --clean",
    "rebuild": "npm run clean&&npm run build",
    "uglify": "node uglify.mjs",
    "copy-native": "node copy-native.js",
    "ugly-build": "npm run clean && npm run build && npm run uglify",
    "test": "jest",
    "coverage": "nyc --clean jest --coverage",
    "fix": "eslint ./**/*.ts --fix"
  },
  "dependencies": {
    "oxc-resolver": "1.11.0-htrunk1.AllScenario.devecostudio.r3",
    "@babel/core": "7.23.0",
    "@babel/plugin-transform-class-properties": "7.22.5",
    "@baize/wdk": "0.7.2",
    "@ohos/hvigor-arkts-base": "6.22.3",
    "@ohos/hvigor-common": "6.22.3",
    "@ohos/hvigor-logger": "6.22.3",
    "@ohos/coverage": "2.0.42",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "24.1.0",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-terser": "0.3.0",
    "@rollup/pluginutils": "5.0.2",
    "glob": "10.5.0",
    "js-beautify": "1.15.3",
    "source-map": "0.7.4"
  },
  "overrides": {
    "electron-to-chromium": "1.4.568",
    "terser": "5.17.1",
    "get-intrinsic": "1.2.7",
    "flatted": "3.2.7",
    "rfdc": "1.3.0",
    "foreground-child": "3.2.1",
    "semver": "7.5.4",
    "postcss": "8.5.3",
    "caniuse-lite": "1.0.30001700",
    "@floating-ui/dom": "1.7.0",
    "@floating-ui/core": "1.7.0",
    "brace-expansion": "2.0.2",
    "@jridgewell/gen-mapping": "0.3.12"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.node",
    "dist/**/*.d.ts",
    "res/**/*"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "author": "huawei",
  "ohos": {
    "org": "huawei"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "keywords": [
    "hvigor",
    "OpenHarmony"
  ]
}
